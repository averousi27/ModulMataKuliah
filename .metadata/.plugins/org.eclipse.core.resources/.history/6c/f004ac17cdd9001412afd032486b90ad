package com.AIS.Modul.MataKuliah.Controller;

import javax.servlet.http.HttpSession;

import com.AIS.Modul.MataKuliah.Model.*;
import com.AIS.Modul.MataKuliah.Controller.*;
import com.AIS.Modul.MataKuliah.Service.*;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.ui.ModelMap;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.servlet.ModelAndView;

@Controller
public class KurikulumController {
	 
	
	//Show kurikulum
	@RequestMapping(value="/kurikulum", method=RequestMethod.GET)
	public String printWelcome(ModelMap model){
		model.addAttribute("message", "This is from Kurikulum Controller");
		return("hello");
	}
	
}
